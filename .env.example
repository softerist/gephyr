# Gephyr API key (required for console scripts).
# Console maps this to API_KEY in the container.
GEPHYR_API_KEY=gph_your_api_key_here

# Google OAuth credentials (required for login flow).
# Accepted aliases in code:
# - client id: GEPHYR_GOOGLE_OAUTH_CLIENT_ID, ABV_GOOGLE_OAUTH_CLIENT_ID, GOOGLE_OAUTH_CLIENT_ID
# - client secret: GEPHYR_GOOGLE_OAUTH_CLIENT_SECRET, ABV_GOOGLE_OAUTH_CLIENT_SECRET, GOOGLE_OAUTH_CLIENT_SECRET
GEPHYR_GOOGLE_OAUTH_CLIENT_ID=your_client_id.apps.googleusercontent.com
GEPHYR_GOOGLE_OAUTH_CLIENT_SECRET=GOCSPX-your_client_secret

# Optional runtime settings (defaults shown)
ABV_AUTH_MODE=strict
ABV_ALLOW_LAN_ACCESS=false
ABV_ENABLE_ADMIN_API=false
ABV_DATA_DIR=
ABV_PUBLIC_URL=
ABV_MAX_BODY_SIZE=104857600
